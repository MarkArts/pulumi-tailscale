{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "acl.ts",
        "config/index.ts",
        "config/vars.ts",
        "deviceAuthorization.ts",
        "deviceSubnetRoutes.ts",
        "dnsNameservers.ts",
        "dnsPreferences.ts",
        "dnsSearchPaths.ts",
        "getDevice.ts",
        "getDevices.ts",
        "index.ts",
        "provider.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
